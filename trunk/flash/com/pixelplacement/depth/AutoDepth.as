package com.pixelplacement.depth{	import flash.display.*;		public class AutoDepth extends MovieClip{		public static function slice(object:MovieClip,amount:uint):void{			var depthPush:uint=0;			for(var i:uint=object.numChildren;i>0;i--){				trace(object.getChildAt(i-1).width,object.getChildAt(i-1).height)				var _width=object.getChildAt(i-1).width				var _height=object.getChildAt(i-1).height				object.getChildAt(i-1).z=depthPush				object.getChildAt(i-1).width=_width				object.getChildAt(i-1).height=_height				depthPush+=amount;			}		}	}}